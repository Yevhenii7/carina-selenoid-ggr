<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="Carina API Demo Tests" parallel="methods">

	<parameter name="suiteOwner" value="qpsdemo"/>
	<parameter name="jenkinsJobName" value="Api-Demo-Test"/>
	<parameter name="jenkinsJobType" value="api"/>
	<parameter name="jenkinsEnvironments" value="DEMO"/>
	<parameter name="jenkinsEmail" value="kolchiba777@gmail.com"/>
  
	<parameter name="jenkinsRegressionPipeline" value="api_regression, full_regression"/>

	<parameter name="jenkinsRegressionMatrix_full_regression" value="browser:chrome; browser:firefox"/>
	<parameter name="jenkinsJobExecutionOrder" value="1"/>
	<parameter name="jenkinsJobExecutionMode" value="continue"/>
	<parameter name="jenkinsAutoScreenshot" value="true"/>

	<test name="User API tests">
		<classes>
			<class name="com.zebrunner.carina.demo.APISampleTest" />
		</classes>
	</test>

</suite>